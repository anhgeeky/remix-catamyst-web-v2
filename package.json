{
  "name": "catamyst-web",
  "version": "0.2.4",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8000",
    "next-lint": "next lint",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 8000",
    "export": "next export",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint-fix": "eslint --fix . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "jest": "jest",
    "test": "npm run lint && npm run type-check",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.9",
    "@chakra-ui/react": "1.3.3",
    "@chakra-ui/skip-nav": "1.1.5",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.3.0",
    "@hookform/resolvers": "2.0.0-beta.3",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "10.2.3",
    "@sentry/browser": "6.4.0",
    "@sentry/integrations": "6.4.0",
    "@sentry/node": "6.4.0",
    "@sentry/react": "6.4.0",
    "@sentry/tracing": "6.4.0",
    "@sentry/webpack-plugin": "1.15.0",
    "@splitbee/web": "0.2.4",
    "@supabase/supabase-js": "1.11.0",
    "@svgr/webpack": "5.5.0",
    "@testing-library/react": "11.2.7",
    "axios": "0.21.1",
    "chakra-ui-autocomplete": "1.3.0",
    "contrast": "1.0.1",
    "dayjs": "1.10.4",
    "dotenv-flow": "3.2.0",
    "escape-html": "1.0.3",
    "framer-motion": "3.5.2",
    "husky": "^4.2.3",
    "immutable": "4.0.0-rc.12",
    "is-url": "1.2.4",
    "lower-case": "2.0.2",
    "next": "11.0.0",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flagkit": "2.0.4",
    "react-hook-form": "6.15.5",
    "react-html-parser": "2.0.2",
    "react-icons": "4.2.0",
    "react-iframe": "^1.8.0",
    "react-redux": "7.2.4",
    "react-remove-scroll": "2.4.2",
    "react-syntax-highlighter": "15.4.3",
    "redux": "4.1.0",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "slate": "0.59.0",
    "slate-history": "0.59.0",
    "slate-hyperscript": "0.59.0",
    "slate-react": "0.59.0",
    "slugify": "1.5.3",
    "swr": "0.5.6",
    "uuid": "8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@hookform/devtools": "2.2.1",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.0",
    "@types/react": "17.0.5",
    "@types/react-redux": "7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-config-next": "11.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "next-sitemap": "1.6.57",
    "prettier": "^2.0.2",
    "redux-devtools-extension": "2.13.9",
    "ts-jest": "27.0.3",
    "typescript": "4.3.4"
  }
}
